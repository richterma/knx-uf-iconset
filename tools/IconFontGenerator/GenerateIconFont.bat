@ECHO off

:: configuration options
SET PROGRAMPATH="C:\Program Files (x86)\FontForgeBuilds\bin\fontforge.exe"
::Examples: "\.", "..\icon_font\", ".\output\", "D:\absolute\path\without\spaces\"
SET OUTDIRECTORY=".\output\"
SET FONTNAME=KNXUF-IconFont
SET FONTNAMETYPE=.ttf
SET LOGFILETYPE=.log

:: actual script
IF EXIST ..\..\optimized_svg\raw_svg\ (
	SET SVGDIRECTORY=..\..\optimized_svg\raw_svg\
) ELSE (
	IF EXIST ..\..\optimized_svg\ (
		SET SVGDIRECTORY=..\..\optimized_svg\
	) ELSE (
		SET SVGDIRECTORY=..\..\raw_svg\
		ECHO It looks like you didn't optimize the svg files.
		ECHO Some of the icons will not display correctly without previous optimization.
		ECHO Check the readme for further information.
		ECHO.
		ECHO Press any button to continue with unoptimized files.
		ECHO.
		PAUSE
	)
)

:: resolve absolute path of font directory.
CALL :NORMALIZEPATH %OUTDIRECTORY%
SET FULLDIR=%RETVAL%
mkdir %FULLDIR% > NUL 2>&1
SET FULLFONTPATH=%FULLDIR%%FONTNAME%%FONTNAMETYPE%
SET FULLLOGPATH=%FULLDIR%%FONTNAME%%LOGFILETYPE%

ECHO Creating output file: %FULLFONTPATH%
break > %FULLLOGPATH%

:: create empty font. If we need to _just_ update an existing file, remove this line.
%PROGRAMPATH% -lang=ff -script fontforge/createEmptyFont.ff %FULLFONTPATH% %FONTNAME% > NUL 2>&1


