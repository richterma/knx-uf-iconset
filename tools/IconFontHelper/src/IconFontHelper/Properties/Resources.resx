  <data name="BatchFooter" xml:space="preserve">
    <value>:: ========== FUNCTIONS ==========
EXIT /B

:UPDATEICON
	CALL :NORMALIZEPATH %SVGDIRECTORY%%2
	SET SVG=%RETVAL%
	ECHO Adding icon number %1: %2
	ECHO Number %1: %2&gt;&gt; %FULLLOGPATH%
	%PROGRAMPATH% -lang=ff -script fontforge/updateGlyph.ff %FULLFONTPATH% %SVG% %1 %3 %4 %5 %6 &gt; NUL 2&gt;&amp;1

	EXIT /B

:NORMALIZEPATH
:: resolve relative to absolute path.
	SET RETVAL=%~dpfn1
	EXIT /B</value>
  </data>
  <data name="BatchHeader" xml:space="preserve">
    <value>@ECHO off

:: configuration options
SET PROGRAMPATH="C:\Program Files (x86)\FontForgeBuilds\bin\fontforge.exe"
::Examples: "\.", "..\icon_font\", ".\output\", "D:\absolute\path\without\spaces\"
SET OUTDIRECTORY=".\output\"
SET FONTNAME=KNXUF-IconFont
SET FONTNAMETYPE=.ttf
SET LOGFILETYPE=.log

:: actual script
IF EXIST ..\..\optimized_svg\raw_svg\ (
	SET SVGDIRECTORY=..\..\optimized_svg\raw_svg\
) ELSE (
	IF EXIST ..\..\optimized_svg\ (
		SET SVGDIRECTORY=..\..\optimized_svg\
	) ELSE (
		SET SVGDIRECTORY=..\..\raw_svg\
		ECHO It looks like you didn't optimize the svg files.
		ECHO Some of the icons will not display correctly without previous optimization.
		ECHO Check the readme for further information.
		ECHO.
		ECHO Press any button to continue with unoptimized files.
		ECHO.
		PAUSE
	)
)

:: resolve absolute path of font directory.
CALL :NORMALIZEPATH %OUTDIRECTORY%
SET FULLDIR=%RETVAL%
mkdir %FULLDIR% &gt; NUL 2&gt;&amp;1
SET FULLFONTPATH=%FULLDIR%%FONTNAME%%FONTNAMETYPE%
SET FULLLOGPATH=%FULLDIR%%FONTNAME%%LOGFILETYPE%

ECHO Creating output file: %FULLFONTPATH%
break &gt; %FULLLOGPATH%

:: create empty font. If we need to _just_ update an existing file, remove this line.
%PROGRAMPATH% -lang=ff -script fontforge/createEmptyFont.ff %FULLFONTPATH% %FONTNAME% &gt; NUL 2&gt;&amp;1

:: update font with each single svg.</value>
  </data>
  <data name="LoggingContent" xml:space="preserve">
    <value>#icon {0}
Select({0})
Import("{1}",0,16)
RemoveOverlap()
CorrectDirection()

bounding = GlyphInfo("BBox")
height = bounding[3] - bounding[1]
factor = ((topPosition - 2 * paddingVertical) / height) * 100
#Scale(factor)

bounding = GlyphInfo("BBox")
deltaX = -bounding[0] + paddingHorizontal
deltaY = (topPosition - paddingVertical - bounding[3])
width = bounding[2] + paddingHorizontal + 10

logText = StrJoin(["GLYPHDETAILS",ToString({0}),ToString(deltaX),ToString(deltaY),ToString(width),ToString(factor)],";")
Print(logText)
#WriteStringToFile(logText + "\\n", logFile, 2)</value>
  </data>
  <data name="LoggingHeader" xml:space="preserve">
    <value># set a few constants
scale = 1024
paddingHorizontal = 10
paddingVertical = 10
topPosition = 820

# create a new font
New()
Reencode("UnicodeFull")
ScaleToEm(scale)</value>
